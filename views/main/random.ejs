<!DOCTYPE html>
<html lang="en">
    <head>
        <%- include('../partials/head') %>
        <%- include('../partials/header') %>
        <title>Random Users</title>
    </head>
    <body>
        <header>Random Users</header>
        <div class="randomtest">
            <% sortUsers.forEach(({name: {first, last}, picture: {large}}) => { %>
                <div class="randomContainer">
                    <img class="random" src="<%= `${large}` %>" />
                    <p><%= `${first} ${last}` %></p>
                </div>
            <% }) %> 


        </div>
        <%- include('../partials/footer') %>
    </body>
</html>
